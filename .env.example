# ATS Buddy Environment Variables
# Copy this file to .env and update with your actual values

# AWS S3 Buckets (automatically set by SAM deployment)
RESUMES_BUCKET=ats-buddy-dev-resumesbucket-123456789
REPORTS_BUCKET=ats-buddy-dev-reportsbucket-123456789

# DynamoDB Table (automatically set by SAM deployment)
RESUME_CACHE_TABLE=ats-buddy-resume-cache-dev

# AWS Configuration
AWS_REGION=ap-southeast-1
BEDROCK_REGION=ap-southeast-1

# Optional: Presigned URL expiration (hours)
PRESIGNED_URL_EXPIRATION_HOURS=24

# Optional: Environment name
ENVIRONMENT=dev

# Note: These values are automatically set when running in AWS Lambda
# For local testing, get the actual values from CloudFormation outputs:
# aws cloudformation describe-stacks --stack-name ats-buddy-dev --query 'Stacks[0].Outputs'